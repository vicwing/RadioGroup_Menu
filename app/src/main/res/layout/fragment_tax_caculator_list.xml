<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/bg_white_f5f5f9"
              android:orientation="vertical">

    <include
        layout="@layout/include_fragment_title"/>


    <ListView
        android:id="@+id/lv_listview"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/form_house_one"
            style="@style/caculate_CommonFormLayout"
            app:hasTopLine="true"
            app:isShotBottomLine="true"
            app:ctitleTextColor="@color/black_33333"
            app:hasRightArrow="false"
            app:titleText="普通住房"/>

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/form_house_two"
            style="@style/caculate_CommonFormLayout"
            app:ctitleTextColor="@color/black_33333"
            app:hasRightArrow="false"
            app:titleText="非普通住房"/>
        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/form_house_three"
            style="@style/caculate_CommonFormLayout"
            android:visibility="gone"
            app:ctitleTextColor="@color/black_33333"
            app:hasRightArrow="false"
            app:titleText="非普通住房"/>
    </LinearLayout>
</LinearLayout>