<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/bg_white_f5f5f9"
              android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_loan"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            app:hasRightArrow="true"/>

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_interest_rate"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            app:hasRightArrow="true"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_fund_loan"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@color/white"
            app:hasRightArrow="true"/>

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_fundloan_rate"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@color/white"
            app:hasRightArrow="true"/>

    </LinearLayout>


    <com.example.cdj.myapplication.cusview.CommonFormLayout
        android:id="@+id/frame_loan_year"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        app:hasRightArrow="true"/>
</LinearLayout>