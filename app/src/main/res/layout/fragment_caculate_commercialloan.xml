<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:CommonFormLayout="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_height="wrap_content">

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_loan"
            style="@style/caculate_CommonFormLayout"/>

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_interest_rate"
            style="@style/caculate_CommonFormLayout"/>

        <com.example.cdj.myapplication.cusview.CommonFormLayout
            android:id="@+id/frame_loan_year"
            style="@style/caculate_CommonFormLayout"/>
    </LinearLayout>



</LinearLayout>