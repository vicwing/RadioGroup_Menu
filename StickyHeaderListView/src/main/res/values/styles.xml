<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/font_black_0</item>
        <item name="colorAccent">@color/font_black_0</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="kProgressLayoutsStyle">@style/qfang_KProgressLayoutStyle</item>
    </style>


    <style name="xpt_horizontal_line">
        <item name="android:background">@color/divide_line_e5e5e5</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>


    <color name="divide_line_e5e5e5">#e5e5e5</color>
</resources>
